{
  "name": "dailyhub/user-main",
  "description": "PWA client API for discount platform (native PHP)",
  "type": "project",
  "require": {
    "php": ">=8.2",
    "vlucas/phpdotenv": "^5.6",
    "firebase/php-jwt": "^6.10",
    "symfony/var-dumper": "^6.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@composer dump-autoload"
    ],
    "post-update-cmd": [
      "@composer dump-autoload"
    ]
  }
}

